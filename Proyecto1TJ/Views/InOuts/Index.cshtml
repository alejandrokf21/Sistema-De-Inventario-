@model IEnumerable<Proyecto1TJ.Models.InOut>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPageBodega.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.fechaInOut)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cantidadInOut)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataBodega.nombreBodega)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tipoInOut1.nombretipoInOut)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.fechaInOut)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cantidadInOut)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataBodega.nombreBodega)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tipoInOut1.nombretipoInOut)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) 
        </td>
    </tr>
}

</table>
